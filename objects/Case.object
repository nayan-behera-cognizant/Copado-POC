    <validationRules>
        <fullName>Agent_can_assign_case_to_Queue</fullName>
        <active>false</active>
        <description>Agent can assign case to queue only: This validation is deactivated as part of PD-12279 ticket</description>
        <errorConditionFormula>NOT(ISBLANK(Owner:User.Id )) &amp;&amp; ($UserRole.Name = &apos;Customer Services Agent Germany&apos; || $UserRole.Name = &apos;Customer Services Agent Japan&apos;) &amp;&amp; Owner:User.Id &lt;&gt; $User.Id &amp;&amp;  ISCHANGED(OwnerId)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>As a Customer Service Agent, you can only assign cases to Queues and not individual users.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Notification_CountryLanguage_Code</fullName>
        <active>true</active>
        <description>To make sure country and langauge codes are correctly filled</description>
        <errorConditionFormula>AND(OR($Profile.Id = $Setup.CustomerServiceProfileId__c.CustomerServiceTeamMemberProfileId__c,$Profile.Id = $Setup.CustomerServiceProfileId__c.CustomerServiceTeamMemberJapanProfileId__c),NOT(OR( 
AND(Notification_Country_Code__c = &apos;JP&apos;, OR(Notification_Language_Code__c = &apos;ja&apos; , Notification_Language_Code__c = &apos;en&apos;)), 
AND(OR(Notification_Country_Code__c = &apos;DE&apos;, Notification_Country_Code__c = &apos;AT&apos;, Notification_Country_Code__c = &apos;CH&apos;) , 
OR(Notification_Language_Code__c = &apos;de&apos; , Notification_Language_Code__c = &apos;en&apos;)) 
)))</errorConditionFormula>
        <errorMessage>Please enter valid Country and Language Codes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Field</fullName>
        <active>false</active>
        <description>Validation to check all required fields are populated while creating and modifying case.</description>
