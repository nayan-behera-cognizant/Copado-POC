    caseToUpdate.OwnerId = &quot;{!$User.Id}&quot;;

    if(&quot;{!Case.Status}&quot; == &quot;Open&quot;){
       caseToUpdate.Status = &quot;In Progress&quot;
    }
    else{
       caseToUpdate.Status = &quot;Being Handled&quot;;
    }  

    var result = sforce.connection.update([caseToUpdate]);

    if(result[0].success == &quot;true&quot;){
        location.reload();
