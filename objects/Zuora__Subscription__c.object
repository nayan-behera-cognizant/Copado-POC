    <fields>
        <fullName>Gift_Code_Remaining_Term__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
    TODAY() &lt; Free_Trial_End_Date__c,
    IF(
        AND( ISNULL( Free_Trial_End_Date__c ), ISNULL( Gift_Code_End_Date__c ) ),
        0,
        ( 
         ( YEAR( Gift_Code_End_Date__c ) * 12 + MONTH( Gift_Code_End_Date__c ) ) -
         ( YEAR( Free_Trial_End_Date__c ) * 12 + MONTH( Free_Trial_End_Date__c ) ) -
         IF( ( DAY( Gift_Code_End_Date__c ) - DAY( TODAY() ) ) &lt; 0, 1, 0 )
        )
    ),
    IF(
        OR(
            AND( ISNULL( Gift_Code_End_Date__c ), ISNULL( Zuora__CustomerAccount__r.Zuora__LastInvoiceDate__c )),
            (Gift_Code_End_Date__c &lt; TODAY())
        ),
        0,
        ( 
         ( YEAR( Gift_Code_End_Date__c ) * 12 + MONTH( Gift_Code_End_Date__c ) ) -
         ( YEAR( Zuora__CustomerAccount__r.Zuora__LastInvoiceDate__c ) * 12 + MONTH( Zuora__CustomerAccount__r.Zuora__LastInvoiceDate__c ) ) -
         IF( ( DAY( Gift_Code_End_Date__c) -  DAY(  TODAY() ) ) &lt; 0, 1, 0 )
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gift Code Remaining Term</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Gift_Code_Start_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
ISBLANK(Giftcode__c), 
null, 
NULLVALUE( 
Free_Trial_End_Date__c, 
IF( 
OR( ISNULL ( Campaign_Id__c ), ISBLANK ( Campaign_Id__c ) ), 
null, 
Zuora__SubscriptionStartDate__c 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gift Code Start Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Gift_Code_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
    Gift_Code_Start_Date__c &gt; TODAY(),
    &apos;Applied&apos;,
    IF(
        AND( Gift_Code_Start_Date__c &lt;= TODAY(), Gift_Code_End_Date__c &gt; TODAY() ),
        &apos;In Use&apos;,
        IF(
            Gift_Code_End_Date__c &lt;= TODAY(),
            &apos;Expired&apos;,
            &apos;&apos;
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gift Code Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
