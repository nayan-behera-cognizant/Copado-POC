    <webLinks>
        <fullName>Anonymise_User_Head_Of_CS</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Anonymise User (Head Of CS)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/apex.js&quot;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js&apos;)} 
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jqueryui/1.10.0/jquery-ui.min.js&apos;)} 

var html = &apos;&lt;div id=&quot;dialog&quot; style=&quot;display: none&quot; title=&quot;Anonymise User&quot;&gt;&lt;p&gt; Are you sure you want to anonymise this customer? If Yes, press &quot;Anonymise&quot;&lt;/p&gt;&lt;/div&gt;&apos;; 

var $g = jQuery.noConflict(); 
$g(function() { 
	$g(&apos;head&apos;).append(&apos;&lt;link rel=&quot;stylesheet&quot; href=&quot;https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css&quot; type=&quot;text/css&quot;/&gt;&apos;); 
	$g(&apos;body&apos;).append(html); 
	$g(&quot;#dialog&quot;).dialog({ 
		autoOpen: true, 
		modal: true, 
		buttons: { 
					&quot;Back&quot;: function() { 
							$g(this).dialog(&quot;close&quot;); 
							}, 
	&quot;Anonymise&quot;: function() { 
	anonymiseUser(); 
			$g(this).dialog(&quot;close&quot;); 
			} 
		} 
	}); 
}); 

function anonymiseUser(){ 

	var accId=&apos;{!Account.Id}&apos;; 
	var idRole =&apos;{!$User.UserRoleId}&apos;; 

	if(idRole != &apos;&apos;){ 	
		var result = sforce.apex.execute(&quot;DeactivateExpiredAccountUser&quot;,&quot;checkRole&quot;, {RoleId :idRole}); 
		if(result == &apos;true&apos;){ 
			var successMsg= sforce.apex.execute(&quot;DeactivateExpiredAccountUser&quot;,&quot;executeDeactivateAccountUser&quot;, {AccountId:accId}); 
				if(successMsg == &apos;Success&apos;){ 
					alert(&apos;{!$Label.LS_Label_ErrMsg_AnonymiseUser_Success}&apos;); 
				} 
				else if(successMsg == &apos;Failed&apos;){ 
					alert(&apos;{!$Label.LS_Label_ErrMsg_AnonymiseUser_Exception}&apos;); 
				} 
				else if (successMsg == &apos;AccountExpired&apos;){ 
					alert(&apos;{!$Label.LS_Label_ErrMsg_AnonymiseUser_Account_Expired}&apos;); 
				} 
		} 
		else{ 
			alert(&apos;{!$Label.LS_Label_ErrMsg_AnonymiseUser_Not_Authorized}&apos;); 
		} 
	} 
	else{ 
		alert(&apos;{!$Label.LS_Label_ErrMsg_AnonymiseUser_No_Role}&apos;); 
	} 
};</url>
