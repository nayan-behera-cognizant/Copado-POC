        <formula>IF(
    Gift_Code_Start_Date__c &gt; TODAY(),
    &apos;Applied&apos;,
    IF(
        AND( Gift_Code_Start_Date__c &lt;= TODAY(), Gift_Code_End_Date__c &gt; TODAY() ),
        &apos;In Use&apos;,
        IF(
