    <rules>
        <fullName>Update case to in progress</fullName>
        <actions>
            <name>Update_Case_status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED( OwnerId ) &amp;&amp; NOT(ISBLANK( Owner:User.Id )) &amp;&amp;  ISPICKVAL(Status, &apos;Open&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update language code on Case</fullName>
        <actions>
            <name>Update_Case_language</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contact.IsPersonAccount</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update not open case to Being_Handled</fullName>
        <actions>
            <name>update_to_Being_Handled</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED( OwnerId ) &amp;&amp; NOT(ISBLANK( Owner:User.Id )) &amp;&amp;  (ISPICKVAL(Status,&apos;Resolved&apos;) || ISPICKVAL(Status,&apos;Waiting for Customer&apos;) ||  ISPICKVAL(Status,&apos;Closed&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
