        <ruleEntry>
            <businessHoursSource>Case</businessHoursSource>
            <escalationAction>
                <assignedTo>Head_Of_CS_Japan</assignedTo>
                <assignedToTemplate>JP_Templates/SUPPORTCaseescalationnotificationSAMPLE</assignedToTemplate>
