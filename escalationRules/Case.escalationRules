                <notifyCaseOwner>true</notifyCaseOwner>
                <notifyToTemplate>JP_Templates/SUPPORTCaseescalationnotificationSAMPLE</notifyToTemplate>
            </escalationAction>
            <escalationStartTime>CaseLastModified</escalationStartTime>
            <formula>AND( ISPICKVAL( Priority , &apos;High&apos;), ISCHANGED( Status ) = false, IsEscalated  = true,  Notification_Country_Code__c  = &apos;JP&apos;, Owner:Queue.QueueName  = &apos;CS Regional Manager Japan&apos; )</formula>
        </ruleEntry>
