<apex:page controller="MyAccountController" showHeader="false" cache="false" expires="0" standardStylesheets="false" action="{!navigateUser}" applyHtmlTag="false" docType="html-5.0">

    <head>
        <title>{!$Label.LS_MyAccount_Title}</title>
        <meta name="google-site-verification" content="1m_JV80vU3gln3KyYeAm5woixeUHQOvhrbGjS-0g6J0" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="description" content="{!$label.LS_MyAccount_Description}" />
        <meta name="keywords" content="{!$label.LS_MyAccount_Keywords}" />
        <apex:includescript value="{!URLFOR($Resource.LS_JQuery)}" />
        <apex:includescript value="{!URLFOR($Resource.DAZN_Styleguide, 'DAZN_Styleguide/js/jquery.min.js')}" />
        <apex:includescript value="{!URLFOR($Resource.Version)}" />
        <!--PD-8766 - To get version of the product from Static Resource-->
        <link REL="SHORTCUT ICON" HREF="{!URLFOR($Resource.DAZN_favicon_default)}" />
        <link REL="apple-touch-icon" sizes="57x57" HREF="{!URLFOR($Resource.DAZN_appletouchicon57_57 )}" />
        <link REL="apple-touch-icon" sizes="60x60" HREF="{!URLFOR($Resource.DAZN_appletouchicon60_60 )}" />
        <link REL="apple-touch-icon" sizes="72x72" HREF="{!URLFOR($Resource.DAZN_appletouchicon72_72 )}" />
        <link REL="apple-touch-icon" sizes="76x76" HREF="{!URLFOR($Resource.DAZN_appletouchicon76_76 )}" />
        <link REL="apple-touch-icon" sizes="114x114" HREF="{!URLFOR($Resource.DAZN_appletouchicon144_144 )}" />
        <link REL="apple-touch-icon" sizes="120x120" HREF="{!URLFOR($Resource.DAZN_appletouchicon120_120 )}" />
        <link REL="apple-touch-icon" sizes="144x144" HREF="{!URLFOR($Resource.DAZN_appletouchicon144_144 )}" />
        <link REL="apple-touch-icon" sizes="152x152" HREF="{!URLFOR($Resource.DAZN_appletouchicon152_152 )}" />
        <link REL="apple-touch-icon" sizes="180x180" HREF="{!URLFOR($Resource.DAZN_appletouchicon180_180 )}" />
        <link REL="icon" type="image/png" HREF="{!URLFOR($Resource.DAZN_favicon_32_32 )}" sizes="32x32" />
        <link REL="icon" type="image/png" HREF="{!URLFOR($Resource.DAZN_androidchrome192_192 )}" sizes="192x192" />
        <link REL="icon" type="image/png" HREF="{!URLFOR($Resource.DAZN_favicon_96_96 )}" sizes="96x96" />
        <link REL="icon" type="image/png" HREF="{!URLFOR($Resource.DAZN_favicon_16_16 )}" sizes="16x16" />
        <link rel="mask-icon" href="{!URLFOR($Resource.DAZN_favicon_svg16_16 )}" color="#000" />
        <link rel="icon" type="image/svg" sizes="any" href="{!URLFOR($Resource.DAZN_favicon_svg16_16 )}" />
        <!-- PD-8777 start -->
        <c:GoogleAnalytics />
        <!-- PD-8777 end -->
    </head>

    <body bgcolor="black" class="myAct">
        <!-- Google Tag Manager -->
        <c:GTMHeader sPageName="myaccount"></c:GTMHeader>
        <!-- End Google Tag Manager -->
        <apex:form id="frmMyAccount" html-autocomplete="false">
            <apex:actionFunction name="saveNameChanges" action="{!saveNameChanges}" reRender="idMyDetail,idName,idErrorMsg" />
            <apex:actionFunction name="saveEmailChanges" action="{!saveEmailChanges}" reRender="idMyDetail,idEmail,idErrorMsg, idEmailInput" oncomplete="displayEmailPwdError();" />
            <apex:actionFunction name="savePasswordChanges" action="{!savePasswordChanges}" reRender="idMyDetail,idPassword,idErrorMsg, idPasswordEdit" />
            <apex:actionFunction name="showSignOutBanner" action="{! showSignOutBanner }" />
            <apex:actionFunction name="SaveLanguageChanges" action="{!updateUserLanguage}" oncomplete="window.location.reload();">
                <apex:param name="language" value="" assignTo="{!strSelectedLanguage}" />
            </apex:actionFunction>
            <apex:actionFunction name="refreshDeviceSection" action="{! showAllUserDevices }" reRender="idMyDevices" oncomplete="return false;" />
            <apex:actionfunction name="RedirectToPauseOrWhy" action="{!naviagateToWhyPage}" />
            <apex:actionfunction name="naviagateToSetPause" action="{!naviagateToSetPause}" />
            <apex:actionfunction name="keepMyDaznActive" action="{!keepMyDaznActive}" />
            <apex:actionfunction name="restartNow" action="{!restartNow}" />
            <!-- Panel for Banner -->
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="row">
                    <apex:outputPanel rendered="{!showbanner}">
                        <div class="alert alert-warning bannerContainer" id="myAlert">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                                    <!--added as a part of PD-11051-->
                                    <h1 class="banner "><apex:outputText escape="false" value="{!HTMLENCODE(sBannerHeader)}" /></h1>
                                    <apex:outputText escape="false" value="{!HTMLENCODE(sBanner)}" styleClass="alertStat CustomLabels" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 signOutAllBtn">
                                    <span class="form-fields--form-button-e53320ec primary frtBtn">
                                            <apex:commandLink value="{!$Label.LS_Button_OK}" onclick="updatedataLayerAttribute(event)" rendered="{!bShowSignOutButton == false}" action="{!closeBanner}" styleClass="wrapper btn txtUpperCase okBtn"/>
                                            <apex:commandLink value="{!$Label.LS_Button_Cancel}" onclick="updatedataLayerAttribute(event)" rendered="{!bShowSignOutButton}" action="{!closeBanner}" styleClass="wrapper btn txtUpperCase okBtn"/>
                                        </span>
                                    <span class="ember-view ember-view form-fields--form-button-e53320ec form-fields--form-button-ae71fbed reverse primary">
                                            <apex:commandLink onclick="updatedataLayerAttribute(event)" value="{!$Label.LS_MyAccount_Banner_Button_Start_Watching_DAZN}" rendered="{!showStartWatchingDAZN}" action="{!DAZNHomePageNavigationHandler}" styleClass="reverse wrapper btn txtUpperCase btn-text startBtn"/>
                                        </span>
                                    <apex:outputPanel rendered="{!bShowSignOutButton}" styleClass="ember-view ember-view form-fields--form-button-e53320ec form-fields--form-button-ae71fbed reverse primary">
                                        <apex:commandLink action="{!signoutAllDevices}" onclick="updatedataLayerAttribute(event)" value="{!$Label.LS_Button_YesDeregister}" styleClass="reverse wrapper btn txtUpperCase btn-text" id="SignOutAllDevices" />
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                </div>
            </div>
            <!-- Panel for Banner for single device-->
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="row">
                    <apex:outputPanel rendered="{!showbannerforSingledevice}">
                        <div class="alert alert-warning bannerContainer" id="myAlert">
                            <div class="row">

                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <h1 class="banner">{!HTMLENCODE(sBannerHeader)} </h1>
                                    <p class="alertStat">{!HTMLENCODE(sBanner)}</p>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-xs-12 col-sm-12 col-md-12 dereg1Dvc">
                                    <span class="form-fields--form-button-e53320ec primary">
                                            <apex:commandLink value="{!$Label.LS_Button_Cancel}" action="{!closeBanner}" styleClass="btn txtUpperCase okBtn cancelBtn"/>
                                        </span>
                                    <apex:outputPanel rendered="{!bShowSignOutButton}" styleClass="ember-view ember-view form-fields--form-button-e53320ec form-fields--form-button-ae71fbed reverse primary">

                                        <apex:commandLink action="{!deregisterSingelUserDevice}" id="btnSignUp1" value="{!$Label.LS_Button_YesDeregister}" styleClass="reverse wrapper btn txtUpperCase btn-text" />
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                </div>
            </div>

            <!-- Pannel for Gift Code Expiry Banner -->
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="row">
                    <apex:outputPanel rendered="{! showGiftCodeExpiryBanner }">
                        <div class="alert alert-warning bannerContainer" id="myAlert">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                                    <!--added as a part of PD-11051-->
                                    <h1 class="banner "><apex:outputText escape="false" value="{! $label.LS_MyAccount_GiftCodeExpiryBanner_Header }" /></h1>
                                    <apex:outputText escape="false" value="{! $Label.LS_MyAccount_GiftCodeExpiryBanner_Body }" styleClass="alertStat CustomLabels" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 dereg1Dvc">
                                    <span class="form-fields--form-button-e53320ec primary">
                                        <apex:commandLink value="{! $Label.LS_MyAccount_GiftCodeExpiryBanner_SkipBtn }" action="{! closeGiftCodeExpiryBanner }" styleClass="btn txtUpperCase okBtn cancelBtn"/>
                                    </span>
                                    <apex:outputPanel styleClass="ember-view ember-view form-fields--form-button-e53320ec form-fields--form-button-ae71fbed reverse primary">
                                        <apex:commandLink action="{! updatePaymentSettings }" value="{! $Label.LS_MyAccount_GiftCodeExpiryBanner_UpdateBtn }" styleClass="reverse wrapper btn txtUpperCase btn-text" style="width: 100% !important; padding: 0 10px 0 10px;" />
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                </div>
            </div>

            <div class="container wrapperMyAccount">
                <c:LS_Header />
                <!-- Panel for subscription status -->
                <apex:outputPanel rendered="{!showResubscribe}"></apex:outputPanel>
                <!-- Username, Email and Password Section -->
                <div class="col-md-12 padding0">
                    <apex:outputPanel id="idMyDetail">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-4 col-lg-4 padding0">
                                    <label for="input-id" class="col-xs-12 col-sm-12 col-md-12 col-lg-12 foundryFontMedium padding0 detailsHdr">{!$Label.LS_MyAccount_Label_My_Details}</label>
                                </div>
                                <div class="col-md-8 col-lg-8 padding0">
                                    <!-- Username display and edit Section -->
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                            <apex:outputPanel id="idName">
                                                <apex:outputPanel rendered="{!NOT(isNameEdit) && NOT(isNameEditForJP)}">
                                                    <div class="row">
                                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 myAccountlabelTxt">
                                                            {!$Label.LS_MyAccount_Name}
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 inptValue">
                                                            <apex:outputText escape="false" Value="{!HTMLENCODE(sAccountName)}"/>
                                                        </div>
                                                        <div class="col-xs-4 col-sm-4 col-md-2 txtUpperCase foundryFontMedium" style="text-align:right;">
                                                            <apex:commandLink value="{!$Label.LS_Button_Edit}" onclick="updatedataLayerAttribute(event)" action="{!edit}" rerender="idName, idEmail, idPassword" styleClass="edit-button link-unique" id="editName">
                                                                <apex:param value="Name" name="field" />
                                                            </apex:commandLink>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel id="idNameInput" rendered="{!isNameEdit}">
                                                    <div class="row foundryFontRegular margBot20 ">
                                                        <div class="mt-row col-xs-12 col-sm-6 FnLnameBlk">
                                                            <div class="bp1-col-4 style-row inputEditable">
                                                                <div id="ember1099" class="ember-view form-fields--text-input-71621ee4 is-passive-content focus-out">
                                                                    <label class="label"><span class="label-text spanMarg"><apex:outputText escape="false" Value="{!$Label.LS_MyAccount_Label_Firstname}"/></span></label>
                                                                    <div class="input-wrapper"></div>
                                                                    <apex:inputText styleclass="form-control customStyle customFormInput" id="idFirstname" style="margin-bottom: 2%" value="{!strFirstName}" onblur="return validateFirstNameNew();"></apex:inputText>
                                                                    <span class="error-message customErrorMessage" id="idFirstNameValidation">
                                                                        <apex:outputText escape="false" value="{!sNameValidFirstNameErrMsg}" styleclass="errMsg" ></apex:outputText>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="mt-row col-xs-12 col-sm-6 FnLnameBlk ln">
                                                            <div class="bp1-col-4 style-row inputEditable lastNameFld">
                                                                <div id="ember1099" class="ember-view form-fields--text-input-71621ee4 is-passive-content focus-out">
                                                                    <label class="label"><span class="label-text spanMarg"><apex:outputText escape="false" Value="{!$Label.LS_MyAccount_Label_Surname}"/></span></label>
                                                                    <div class="input-wrapper"></div>
                                                                    <apex:inputText styleclass="form-control customStyle customFormInput" id="idSurname" style="margin-bottom: 2%" value="{!strLastName}" onblur="return validateSurNameNew();"></apex:inputText>
                                                                    <span class="error-message customErrorMessage" id="idSurNameValidation">
                                                                        <apex:outputText escape="false" value="{!sNameValidSurNameErrMsg}"></apex:outputText>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="mt-row hidden-xs col-sm-6 FnLnameBlk">&nbsp;</div>
                                                        <div class="mt-row col-xs-12 col-sm-16  FnLnameBlk ln canSavBtn">
                                                            <ul class="cancelSaveSec">
                                                                <li class="sveBtnNew">
                                                                    <apex:outputPanel styleClass="ember-view form-fields--form-button-e53320ec primary">
                                                                        <apex:commandLink value="{!$Label.LS_Button_Save}" onclick="return validateNSaveNameChanges(event, this);" rerender="idName" styleClass="wrapper btn txtUpperCase customStyleBtn saveName" id="SaveName" />
                                                                        <div id="preloadSpinner"></div>
                                                                    </apex:outputPanel>
                                                                </li>
                                                                <li class="cancelBtnNew">
                                                                    <apex:commandLink value="{!$Label.LS_Button_Cancel}" action="{!cancelEdit}" onclick="updatedataLayerAttribute(event)" rerender="idName" styleClass="cancelLink link-unique foundryFontMedium" id="NameCancel">
                                                                        <apex:param value="Name" name="field" />
                                                                    </apex:commandLink>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel id="idNameInputForJPJA" rendered="{!isNameEditForJP}">
                                                        <div class="mt-row col-xs-12 col-sm-6 FnLnameBlk">
                                                            <div class="bp1-col-4 style-row inputEditable">
                                                                <div id="ember1099" class="ember-view form-fields--text-input-71621ee4 is-passive-content focus-out">
                                                                    <label class="label"><span class="label-text spanMarg"><apex:outputText escape="false" Value="{!$Label.LS_MyAccount_Label_Surname}"/></span></label>
                                                                    <div class="input-wrapper"></div>
                                                                    <apex:inputText styleclass="form-control customStyle customFormInput" id="idSurnameJPJA" style="margin-bottom: 2%" value="{!strLastName}" onblur="return validateSurNameNew();"></apex:inputText>
                                                                    <span class="error-message customErrorMessage" id="idSurNameValidation">
                                                                        <apex:outputText escape="false" value="{!sNameValidSurNameErrMsg}"></apex:outputText>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    <div class="row foundryFontRegular margBot20 ">                                                    
                                                        <div class="mt-row col-xs-12 col-sm-6 FnLnameBlk ln">
                                                            <div class="bp1-col-4 style-row inputEditable lastNameFld">
                                                                <div id="ember1099" class="ember-view form-fields--text-input-71621ee4 is-passive-content focus-out">
                                                                    <label class="label"><span class="label-text spanMarg"><apex:outputText escape="false" Value="{!$Label.LS_MyAccount_Label_Firstname}"/></span></label>
                                                                    <div class="input-wrapper"></div>
                                                                    <apex:inputText styleclass="form-control customStyle customFormInput" id="idFirstnameJPJA" style="margin-bottom: 2%" value="{!strFirstName}" onblur="return validateFirstNameNew();"></apex:inputText>
                                                                    <span class="error-message customErrorMessage" id="idFirstNameValidation">
                                                                        <apex:outputText escape="false" value="{!sNameValidFirstNameErrMsg}" styleclass="errMsg" ></apex:outputText>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="mt-row hidden-xs col-sm-6 FnLnameBlk">&nbsp;</div>
                                                        <div class="mt-row col-xs-12 col-sm-16  FnLnameBlk ln canSavBtn">
                                                            <ul class="cancelSaveSec">
                                                                <li class="sveBtnNew">
                                                                    <apex:outputPanel styleClass="ember-view form-fields--form-button-e53320ec primary">
                                                                        <apex:commandLink value="{!$Label.LS_Button_Save}" onclick="return validateNSaveNameChanges(event, this);" rerender="idName" styleClass="wrapper btn txtUpperCase customStyleBtn saveName" id="SaveNameJPJA" />
                                                                        <div id="preloadSpinner"></div>
                                                                    </apex:outputPanel>
                                                                </li>
                                                                <li class="cancelBtnNew">
                                                                    <apex:commandLink value="{!$Label.LS_Button_Cancel}" action="{!cancelEdit}" onclick="updatedataLayerAttribute(event)" rerender="idName" styleClass="cancelLink link-unique foundryFontMedium" id="NameCancelJPJA">
                                                                        <apex:param value="Name" name="field" />
                                                                    </apex:commandLink>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                            </apex:outputPanel>
                                        </div>
                                    </div>
                                    <div class="row emptyRow"> &nbsp; </div>
                                    <!-- Email display and edit Section -->
                                    <div class="row foundryFontMedium">
                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                            <apex:outputPanel id="idEmail">
                                                <apex:outputPanel rendered="{!NOT(isEmailEdit)}">
                                                    <div class="row foundryFontMedium">
                                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 myAccountlabelTxt">
                                                            {!$Label.LS_MyAccount_EmailAddress}
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 inptValue">
                                                            <span class="mailLink"><apex:outputField value="{!oAccount.PersonEmail}"/></span>
                                                        </div>
                                                        <div class="col-xs-4 col-sm-4 col-md-2 txtUpperCase foundryFontMedium" style="text-align:right">
                                                            <apex:commandLink value="{!$Label.LS_Button_Edit}" action="{!edit}" onclick="updatedataLayerAttribute(event)" rerender="idEmail, idName, idPassword" styleClass="edit-button link-unique" id="emailEdit">
                                                                <apex:param value="Email" name="field" />
                                                            </apex:commandLink>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel id="idEmailInput" rendered="{!isEmailEdit}">
                                                    <div class="row foundryFontMedium">
                                                        <div class="col-xs-12 emailPwdFlds">
                                                            <div id="ember1037" class="ember-view form-fields--text-input-71621ee4 is-passive focus-out">
                                                                <label class="label">
                                                                    <span class="label-text">{!$Label.LS_MyAccount_EmailAddress}</span>
                                                                </label>
                                                                <div class="input-wrapper"></div>
                                                                <apex:inputText styleclass="form-control customFormInput emailFld" html-autocomplete="off" value="{!strNewEmail}" id="email" onblur="return validateEmail();" />
                                                                <span class="error-message customErrorMessage" id="emailvalidation">
                                                                <apex:outputText id="idEmailError" value="{!sEmailProvideNewEmailErrorMsg}"></apex:outputText>
                                                            </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row margBot20">
                                                        <div class="col-xs-12 emailPwdFlds">
                                                            <div id="ember1037" class="ember-view form-fields--text-input-71621ee4 is-passive focus-out">
                                                                <label class="label">
                                                                    <span class="label-text">{!$Label.LS_MyAccount_Label_Password}</span>
                                                                </label>
                                                                <div class="input-wrapper"></div>
                                                                <apex:inputSecret styleclass="form-control customFormInput" value="{!strPassword}" id="idEmailPwd" onblur="return validateEmailPassword();" onfocus="return validateEmailPasswordFocus();" />
                                                                <div class="col-xs-8 col-sm-8 col-md-12 col-lg-12 errorpanelwithforgotpasserr" id="divemailPwdValidation">
                                                                    <span class="error-message customErrorMessage" id="emailPwdValidation">
                                                                    <apex:outputText id="idEmailPasswordError" value="{!sEmailIncorrectPasswordErrorMsg}"></apex:outputText>
                                                                </span>
                                                                </div>
                                                                <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 ember-view foundryFontMedium errorpanelwithforgotpasslink">
                                                                    <div class="displayBlock margBot20 errorpanelwithforgotpass ">
                                                                        <a id="aForgotPassword" href="{!sForgotPasswordURL}" class="link-unique" onclick="updatedataLayerAttribute(event)">{!$Label.LS_MyAccount_Link_ForgotPassword}</a>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="hidden-xs hidden-sm hidden-md hidden-lg">&nbsp;</div>
                                                        <div class="col-xs-12 emailPwdFlds foundryFontMedium ember-view form-fields--form-button-e53320ec savecancel saveemailpwd">
                                                            <ul class="cancelSaveSec">
                                                                <li class="sveBtnNew">
                                                                    <apex:outputPanel styleClass="ember-view form-fields--form-button-e53320ec primary">
                                                                        <apex:commandLink id="btnSaveEmail" value="{!$Label.LS_Button_Save}" onclick="return validateNSaveEmailChanges(event, this);" rerender="idEmail" styleClass="wrapper btn txtUpperCase customStyleBtn saveEmail" />
                                                                        <div id="preloadSpinner"></div>
                                                                    </apex:outputPanel>
                                                                </li>
                                                                <li class="cancelBtnNew">
                                                                    <apex:commandLink value="{!$Label.LS_Button_Cancel}" action="{!cancelEdit}" rerender="idEmail" onclick="updatedataLayerAttribute(event)" styleClass="customSpanStyle cancelLink link-unique foundryFontMedium" id="EmailCancel">
                                                                        <apex:param value="Email" name="field" />
                                                                    </apex:commandLink>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                            </apex:outputPanel>
                                        </div>
                                    </div>
                                    <div class="row emptyRow"> &nbsp; </div>
                                    <!-- Password display and edit Section -->
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                            <apex:outputPanel id="idPassword">
                                                <apex:outputPanel rendered="{!NOT(isPasswordEdit)}">
                                                    <div class="row">
                                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 txtUpperCase myAccountlabelTxt">
                                                            {!$Label.LS_MyAccount_Label_Password}
                                                        </div>
                                                    </div>
                                                    <div class="row ">
                                                        <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 margintop inptValuePwd">
                                                            <apex:outputText value="{!$Label.LS_MyAccount_Placeholder_EncryptedPassword}" />
                                                        </div>
                                                        <div class="col-xs-4 col-sm-4 col-md-2 margBot20 foundryFontMedium" style="text-align:right">
                                                            <apex:commandLink value="{!$Label.LS_Button_Change}" onclick="updatedataLayerAttribute(event)" action="{!edit}" reRender="idPassword, idName, idEmail" styleClass="edit-button link-unique" id="PasswordChange">
                                                                <apex:param value="Password" name="field" />
                                                            </apex:commandLink>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel id="idPasswordEdit" rendered="{!isPasswordEdit}">
                                                    <div class="row">
                                                        <div class="col-xs-12 emailPwdFlds editPwd">
